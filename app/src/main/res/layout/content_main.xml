<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/myLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#000000">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="25pt"
        android:orientation="horizontal">
    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="100pt"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView_nothing0"
            android:layout_width="40pt"
            android:layout_height="match_parent"
            android:text=""
            android:textColor="#FFFFFF"
            android:textStyle="bold"/>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="50pt"
            android:layout_height="100pt"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image_d1"
                android:layout_width="50pt"
                android:layout_height="50pt"
                app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView_nothing1"
            android:layout_width="10pt"
            android:layout_height="match_parent"
            android:text=""
            android:textColor="#FFFFFF"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/textView_D1"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:text="TextView_D1"
            android:textColor="#FFFFFF"
            android:textStyle="bold"/>
    </LinearLayout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="102pt"
        android:orientation="horizontal">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="40pt"
            android:layout_height="142pt"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView_nothing2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text=""
                android:textColor="#FFFFFF"
                android:textStyle="bold"/>


        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="40pt"
        android:layout_height="40pt"
        android:orientation="horizontal">

        <Spinner
            android:id="@+id/spinnner"
            android:layout_width="40pt"
            android:layout_height="40pt"
            android:layout_weight="1"
            android:background="@android:drawable/checkbox_off_background"></Spinner>
    </LinearLayout>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="140pt"
        android:layout_height="23pt"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/image_d2"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

        <ImageView
            android:id="@+id/image_d3"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />
        <ImageView
            android:id="@+id/image_d4"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />
        <ImageView
            android:id="@+id/image_d5"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />
        <ImageView
            android:id="@+id/image_d6"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

    </LinearLayout>




</LinearLayout>

